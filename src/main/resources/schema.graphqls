type Query {
    getBooks(id: ID, name: String): [Book]
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author(firstName: String): [Author]
}

type Author {
    id: ID
    firstName: String
    lastName: String
    hobbies(hobbieName: String): [Hobbies]
}

type Hobbies {
    id: ID
    name: String
}
