type Query {
    getBooks(id: ID, name: String): [Book]
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: [Author]
}

type Author {
    id: ID
    firstName: String
    lastName: String
    book: [Book]
    hobbies(filter: String): [Hobbies]!
}

type Hobbies {
    id: ID
    name: String
}
